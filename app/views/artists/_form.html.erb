<%= form_for(@artist) do |f| %>
  <%= render partial: "shared/errors", locals: { target: @artist } %>

  <%= f.label :name, "Artist name:"  %>
  <%= f.text_field :name %>
  <br>
  <%= f.label :image_path, "Add an image:"%>
  <%= f.text_field :image_path %>
  <br
  <%= f.submit class: "button" %>
<% end %>
